<?xml version='1.1' encoding='UTF-8'?>
<com.cloudbees.plugins.credentials.SystemCredentialsProvider plugin="credentials@2.6.1">
  <domainCredentials>
    <com.cloudbees.plugins.credentials.domains.Domain>
      <specifications/>
      <credentials>
        <!-- Snowflake Credentials -->
        <com.cloudbees.plugins.credentials.impl.UsernamePasswordCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>snowflake-account</id>
          <description>Snowflake Account</description>
          <username>your-account</username>
          <password>your-account-name</password>
        </com.cloudbees.plugins.credentials.impl.UsernamePasswordCredentialsImpl>
        
        <com.cloudbees.plugins.credentials.impl.UsernamePasswordCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>snowflake-user</id>
          <description>Snowflake User</description>
          <username>your-username</username>
          <password>your-username</password>
        </com.cloudbees.plugins.credentials.impl.UsernamePasswordCredentialsImpl>
        
        <com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>snowflake-password</id>
          <description>Snowflake Password</description>
          <secret>your-password</secret>
        </com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
        
        <com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>snowflake-role</id>
          <description>Snowflake Role</description>
          <secret>ACCOUNTADMIN</secret>
        </com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
        
        <com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>snowflake-database</id>
          <description>Snowflake Database</description>
          <secret>gravitational_wave_pool</secret>
        </com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
        
        <com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>snowflake-warehouse</id>
          <description>Snowflake Warehouse</description>
          <secret>development_warehouse</secret>
        </com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
        
        <com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>snowflake-schema</id>
          <description>Snowflake Schema</description>
          <secret>raw</secret>
        </com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
        
        <!-- Astronomer Credentials -->
        <com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>astronomer-token</id>
          <description>Astronomer API Token</description>
          <secret>your-astronomer-token</secret>
        </com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
        
        <!-- Docker Credentials -->
        <com.cloudbees.plugins.credentials.impl.UsernamePasswordCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>docker-username</id>
          <description>Docker Hub Username</description>
          <username>your-docker-username</username>
          <password>your-docker-password</password>
        </com.cloudbees.plugins.credentials.impl.UsernamePasswordCredentialsImpl>
        
        <com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>docker-password</id>
          <description>Docker Hub Password</description>
          <secret>your-docker-password</secret>
        </com.cloudbees.plugins.credentials.impl.StringCredentialsImpl>
        
        <!-- GitHub Credentials -->
        <com.cloudbees.plugins.credentials.impl.UsernamePasswordCredentialsImpl>
          <scope>GLOBAL</scope>
          <id>github-credentials</id>
          <description>GitHub Credentials</description>
          <username>your-github-username</username>
          <password>your-github-token</password>
        </com.cloudbees.plugins.credentials.impl.UsernamePasswordCredentialsImpl>
      </credentials>
    </com.cloudbees.plugins.credentials.domains.Domain>
  </domainCredentials>
</com.cloudbees.plugins.credentials.SystemCredentialsProvider>
